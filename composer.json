{
    "name": "run_as_root/magento2-prometheus-exporter",
    "description": "Magento2 Prometheus Exporter",
    "version": "3.1.2",
    "type": "magento2-module",
    "license": "MIT",
    "homepage": "https://github.com/run-as-root/magento2-prometheus-exporter",
